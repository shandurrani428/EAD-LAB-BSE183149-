<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <servlet>
        <servlet-name>lab05Servlet</servlet-name>
        <servlet-class> Servlets.lab05Servlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>lab05Servlet</servlet-name>
        <url-pattern>/lab05</url-pattern>
    </servlet-mapping>
    <filter>
        <filter-name>lab05Filter</filter-name>
        <filter-class>Servlets.lab05Filter</filter-class>
    </filter>
    <filter-mapping>
        <filter-name>lab05Filter</filter-name>
        <url-pattern>/lab05</url-pattern>
    </filter-mapping>
</web-app>